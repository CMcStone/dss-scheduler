<h1>Agenda</h1>

<table class="table table-striped table-condensed">
  <tr>
    <th>Resource</th>
    <th>Date</th>
    <th></th>
  </tr>

<% @reservations.each do |reservation| %>
  <tr id="<%= 'reservation_' + reservation.id.to_s %>">
    <td><%= reservation.title %></td>
    <td><%= reservation.cache_start_time.strftime("%m/%d/%Y") %></td>
    <td><%= link_to 'Cancel', reservation, method: :delete, remote: true %></td>
  </tr>
<% end %>
</table>
